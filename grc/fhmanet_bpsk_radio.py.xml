<block>
  <name>BPSK FHSS Modem</name>
  <key>bpsk_radio</key>
  <category>gr-fhmanet</category>
  <import>from bpsk_radio import bpsk_radio  # grc-generated hier_block</import>
  <make>bpsk_radio(
    sps=$sps,
    access_code_threshold=$access_code_threshold,
)</make>
  <callback>set_sps($sps)</callback>
  <callback>set_access_code_threshold($access_code_threshold)</callback>
  <param>
    <name>Samples/symbol</name>
    <key>sps</key>
    <value>8</value>
    <type>raw</type>
  </param>
  <param>
    <name>Access code threshold</name>
    <key>access_code_threshold</key>
    <value>0</value>
    <type>raw</type>
  </param>
  <sink>
    <name>bb_in</name>
    <type>complex</type>
    <vlen>1</vlen>
  </sink>
  <sink>
    <name>freq_in</name>
    <type>message</type>
  </sink>
  <sink>
    <name>msg_in</name>
    <type>message</type>
  </sink>
  <source>
    <name>bb_out</name>
    <type>complex</type>
    <vlen>1</vlen>
  </source>
  <source>
    <name>msg_out</name>
    <type>message</type>
  </source>
